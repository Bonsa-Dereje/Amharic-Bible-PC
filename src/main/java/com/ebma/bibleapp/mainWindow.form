                <Component class="javax.swing.JComboBox" name="verseChooser">
                  <Properties>
                    <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
                      <Font name="Nokia Pure Headline Ultra Light" size="14" style="0"/>
